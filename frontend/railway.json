
{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm install && npm run build"
  },
  "start": {
    "cmd": "npm run preview"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "healthcheckPath": "/"
  },
  "ports": [
    {
      "port": 4173,
      "protocol": "HTTP"
    }
  ]
}

